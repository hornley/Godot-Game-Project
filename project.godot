config_version=5

[application]

config/name="Farm Game"
run/main_scene="res://scenes/ui/game_menu_screen.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio/game_audio_bus_layout.tres"

[autoload]

Util="*res://Singletons/Util.gd"
GameManager="*res://Singletons/game_manager.gd"
PlayerManager="*res://Singletons/player_manager.gd"
HotbarManager="*res://Singletons/hotbar_manager.gd"
GameDataManager="*res://Singletons/game_data_manager.gd"
DayAndNightCycleManager="*res://Singletons/day_and_night_cycle_manager.gd"
GameDialogueManager="*res://Singletons/game_dialogue_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

